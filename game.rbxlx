local PubTypes = require(Package.PubTypes)

type Set&lt;T> = {[T]: any}
type Descendant = (PubTypes.Dependent &amp; PubTypes.Dependency) | PubTypes.Dependent

local function updateAll(ancestor: PubTypes.Dependency)
